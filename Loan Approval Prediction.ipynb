    go.Bar(
        x=gender_analysis.index,
        y=gender_analysis['Total_Loans'],
        name='Total Loans',
        marker_color=colors,
        text=gender_analysis['Total_Loans'],
        texttemplate='%{text:,}',
        textposition='auto',
        marker_line=dict(width=2, color='darkgray')
    ),
    row=1, col=2
)

# 3. Scatter Plot: Default Rate vs Total Loans
